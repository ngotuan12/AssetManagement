	<queryString>
		<![CDATA[SELECT
(SELECT name FROM asset WHERE id=(SELECT parent_id FROM asset WHERE id=(SELECT parent_id FROM asset WHERE id=c.asset_id))) Nhom1,
(SELECT name FROM asset WHERE id=(SELECT parent_id FROM asset WHERE id=c.asset_id)) Nhom2,
(SELECT name FROM asset WHERE id=c.asset_id) chi_tieu,
(SELECT code FROM asset WHERE id=c.asset_id) ma_so,
c.serial,c.capital_id,c.use_date,
(select name from list where list_type='4' and id=c.state_id) status,
   null unit,(select supplier_name from supplier where  id=c.supplier_id) supplier,
   original_value acc_original,remain_value acc_remain, original_value inv_original ,check_remain_amount inv_remain,
  0 diff_original,check_remain_amount-remain_value diff_remain,'' note
  FROM stock_asset_serial c left join check_stock_asset_serial d on c.id=d.stock_asset_serial_id
 and ($P{p_capital_id}='-1' or c.capital_id=$P{p_capital_id})
  and ($P{p_status}='-1' or c.state_id=$P{p_status})
  and ($P{p_use_date} is null or c.use_date>=trunc(to_date($P{p_use_date},'yyyy-mm-dd')))
  order by nhom1,nhom2]]>
	</queryString>
