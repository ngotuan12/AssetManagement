	<queryString>
		<![CDATA[select  a.code,a.name,a.parent_code,(select name from asset where code=a.parent_code) parent_name,b.serial,b.capital_id,b.use_date,
    (select name from list where list_type='4' and id=b.state_id) status,
   null unit,(select supplier_name from supplier where  id=b.supplier_id) supplier,
   original_value acc_original,remain_value acc_remain, original_value inv_original ,check_remain_amount inv_remain,
  0 diff_original,check_remain_amount-remain_value diff_remain
  from asset a,(select * from stock_asset_serial c left join check_stock_asset_serial d on c.id=d.stock_asset_serial_id) b
  where a.id=b.asset_id
 and a.parent_code is not null
 and ($P{p_capital_id} is null or b.capital_id=$P{p_capital_id})
  and ($P{p_status} is null or b.state_id=$P{p_status})
  and ($P{p_use_date} is null or b.use_date>=trunc($P{p_use_date}))
  order by a.code
 ]]>
	</queryString>
